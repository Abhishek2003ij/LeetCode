class Solution {
    private void dfs(int ind, List<List<Integer>> rooms, boolean[] vis )
    {
        vis[ind]=true;
        for(int node:rooms.get(ind))
        {
            if(!vis[node])
            {
                   dfs(node,rooms,vis);
            }
        }
    }
    public boolean canVisitAllRooms(List<List<Integer>> rooms) {
     int n = rooms.size();
     boolean[] vis = new boolean[n];
     dfs(0,rooms,vis);
     for(int i=0;i<n;i++)
     {
        if(!vis[i]) return false;
     }
     return true;
        
    }
}
